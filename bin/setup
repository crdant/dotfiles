#!/bin/sh

SELF_PATH=$(cd -P -- "$(dirname -- "$0")" && pwd -P) && SELF_PATH=$SELF_PATH/$(basename -- "$0")
DOTFILES=`dirname $SELF_PATH`

echo ln -s $DOTFILES/ackrc $HOME/.ackrc
echo ln -s $DOTFILES/emacs $HOME/.emacs
echo ln -s $DOTFILES/ssh/config $HOME/.ssh/config
echo ln -s $DOTFILES/zshrc $HOME/.zshrc
echo ln -s $DOTFILES/zshenv $HOME/.zshenv